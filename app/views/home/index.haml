.container

  %h2 Courses list

  %ol
    -@courses.each do |course|
      %li
        = course.title
        = link_to 'Edit', edit_course_path(course)
        = link_to 'Show', course_path(course)
  =link_to 'Add new course', new_course_path
  %hr

  %h2 Groups list

  %ol
    -@groups.each do |group|
      %li
        = group.name
        = link_to 'Edit', edit_group_path(group)
        = link_to 'Show', group_path(group)
  =link_to 'Add new group', new_group_path
  %hr

  %h2 Users list
  %ol
    -@users.each do |user|
      %li
        = user.email
        = link_to 'Edit', edit_user_registration_path(user)
  -#=link_to 'Add new user', new_user_registration_path